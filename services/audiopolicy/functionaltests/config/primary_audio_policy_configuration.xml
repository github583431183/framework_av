<?xml version='1.0' encoding='UTF-8'?>
<module halVersion="6.0" name="primary">
    <attachedDevices>
        
        <item>builtin_mic</item>
        <item>fm_tuner</item>
        <item>Stub-In</item>

        
        <item>speaker</item>
        <item>bus0_usage_main</item>
        <item>bus1_usage_nav_guidance</item>
        <item>bus2_usage_oem</item>
        <item>bus3_usage_system</item>
        <item>bus4_usage_voice</item>
    </attachedDevices>

    <defaultOutputDevice>bus0_usage_main</defaultOutputDevice>
    <mixPorts>
        <mixPort name="primary_input" role="sink">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO,AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_FRONT_BACK" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="8000,11025,16000,44100,48000" />
            </mixPort>
        <mixPort name="radio_input" role="sink">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>

        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_bus0_usage_main_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_speaker_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_bus1_usage_nav_guidance_output" role="source">
             <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
             </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_bus2_usage_oem_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_bus3_usage_system_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_DIRECT|AUDIO_OUTPUT_FLAG_HW_AV_SYNC" name="mixport_bus3_usage_system_direct_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="mixport_bus4_usage_voice_output" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_OUTPUT_FLAG_PRIMARY" name="hfp_tx_mix" role="source">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort flags="AUDIO_INPUT_FLAG_PRIMARY" name="hfp_rx_mix" role="sink">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            </mixPort>
        <mixPort name="mixport_fm_sw" role="sink">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO,AUDIO_CHANNEL_IN_MONO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="8000,11025,16000,44100,48000" />
            </mixPort>
        <mixPort name="mixport_fm_hw" role="sink">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO,AUDIO_CHANNEL_IN_MONO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="8000,11025,16000,44100,48000" />
            </mixPort>
    </mixPorts>

    <devicePorts>
        
        <devicePort address="bottom" role="source" tagName="builtin_mic" type="AUDIO_DEVICE_IN_BUILTIN_MIC">
            <profile channelMasks="AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
        </devicePort>
        <devicePort address="tuner0" role="source" tagName="fm_tuner" type="AUDIO_DEVICE_IN_FM_TUNER">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="600" minValueMB="-3200" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" />
            </gains>
        </devicePort>
        <devicePort address="STUB_IN" role="source" tagName="Stub-In" type="AUDIO_DEVICE_IN_STUB">
            <profile channelMasks="AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
        </devicePort>

        
        <devicePort address="BUS00_USAGE_MAIN" role="sink" tagName="bus0_usage_main" type="AUDIO_DEVICE_OUT_BUS">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                
                <gain defaultValueMB="0" maxValueMB="0" minValueMB="-2400" mode="AUDIO_GAIN_MODE_JOINT" name="volume_bus0_usage_main" stepValueMB="1" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="BUS01_USAGE_NAV_GUIDANCE" role="sink" tagName="bus1_usage_nav_guidance" type="AUDIO_DEVICE_OUT_BUS">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="0" minValueMB="-2400" mode="AUDIO_GAIN_MODE_JOINT" name="volume_bus1_usage_nav_guidance" stepValueMB="1" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="BUS02_USAGE_OEM" role="sink" tagName="bus2_usage_oem" type="AUDIO_DEVICE_OUT_BUS">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="volume_bus2_usage_oem" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="BUS03_USAGE_SYSTEM" role="sink" tagName="bus3_usage_system" type="AUDIO_DEVICE_OUT_BUS">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="volume_bus3_usage_system" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="BUS04_USAGE_VOICE" role="sink" tagName="bus4_usage_voice" type="AUDIO_DEVICE_OUT_BUS">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="volume_bus4_usage_voice" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>

        <devicePort address="SPEAKER" role="sink" tagName="speaker" type="AUDIO_DEVICE_OUT_SPEAKER">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="0" minValueMB="-2400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="1" />
            </gains>
        </devicePort>
        <devicePort address="hfp_client_out" role="sink" tagName="telephony_tx" type="AUDIO_DEVICE_OUT_TELEPHONY_TX">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="hfp_client_in" role="source" tagName="telephony_rx" type="AUDIO_DEVICE_IN_TELEPHONY_RX">
            <profile channelMasks="AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="hfp_client_out_hw" role="sink" tagName="telephony_tx_hw" type="AUDIO_DEVICE_OUT_TELEPHONY_TX">
            <profile channelMasks="AUDIO_CHANNEL_OUT_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="hfp_client_in_hw" role="source" tagName="telephony_rx_hw" type="AUDIO_DEVICE_IN_TELEPHONY_RX">
            <profile channelMasks="AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" useForVolume="true" />
            </gains>
        </devicePort>
        <devicePort address="FM_SW" role="source" tagName="fm_sw" type="AUDIO_DEVICE_IN_FM_TUNER">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="0" minValueMB="-2400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" />
            </gains>
        </devicePort>
        <devicePort address="FM_HW" role="source" tagName="fm_hw" type="AUDIO_DEVICE_IN_FM_TUNER">
            <profile channelMasks="AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
              <gains>
                 <gain defaultValueMB="0" maxValueMB="4000" minValueMB="-8400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" />
             </gains>
        </devicePort>

        <devicePort address="hdmi_in_mic" role="source" tagName="Hdmi-In Mic" type="AUDIO_DEVICE_IN_HDMI">
            <profile channelMasks="AUDIO_CHANNEL_IN_MONO,AUDIO_CHANNEL_IN_STEREO" format="AUDIO_FORMAT_PCM_16_BIT" name="" samplingRates="48000" />
            <gains>
                <gain defaultValueMB="0" maxValueMB="0" minValueMB="-2400" mode="AUDIO_GAIN_MODE_JOINT" name="" stepValueMB="100" />
            </gains>
        </devicePort>
    </devicePorts>

    
    <routes>
        <route sink="primary_input" sources="builtin_mic,Hdmi-In Mic,Stub-In" type="mix" />
        <route sink="radio_input" sources="fm_tuner" type="mix" />
        <route sink="speaker" sources="mixport_speaker_output,fm_hw,mixport_bus0_usage_main_output" type="mix" />
        <route sink="bus0_usage_main" sources="mixport_bus0_usage_main_output,fm_hw" type="mix" />
        <route sink="bus1_usage_nav_guidance" sources="mixport_bus1_usage_nav_guidance_output,fm_hw" type="mix" />
        <route sink="bus2_usage_oem" sources="mixport_bus2_usage_oem_output,fm_hw" type="mix" />
        <route sink="bus3_usage_system" sources="mixport_bus3_usage_system_output,mixport_bus3_usage_system_direct_output,fm_hw" type="mix" />
        <route sink="bus4_usage_voice" sources="mixport_bus4_usage_voice_output,fm_hw,telephony_rx_hw" type="mix" />
        <route sink="telephony_tx" sources="hfp_tx_mix" type="mix" />
        <route sink="telephony_tx_hw" sources="builtin_mic" type="mix" />
        <route sink="hfp_rx_mix" sources="telephony_rx" type="mix" />
        <route sink="mixport_fm_sw" sources="fm_sw,Stub-In" type="mix" />
        <route sink="mixport_fm_hw" sources="fm_hw" type="mix" />
    </routes>

    
    </module>