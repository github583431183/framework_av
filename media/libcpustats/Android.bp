package {
    // A large-scale-change added 'default_applicable_licenses' to import
    // the below license kinds from "frameworks_av_license":
    //   SPDX-license-identifier-Apache-2.0
    default_applicable_licenses: ["frameworks_av_license"],
}

cc_library_static {
    name: "libcpustats",

    srcs: [
        "CentralTendencyStatistics.cpp",
        "ThreadCpuUsage.cpp",
    ],

    local_include_dirs: [
        "include",
    ],

    export_include_dirs: [
        "include",
    ],

    cflags: [
        "-Werror",
        "-Wall",
    ],

    host_supported: true,

    target: {
        darwin: {
            enabled: false,
        },
    },

}
