// audio preprocessing unit test
cc_test {
    name: "AudioPreProcessingTest",

    vendor: true,

    relative_install_path: "soundfx",

    srcs: ["PreProcessingTest.cpp"],

    shared_libs: [
        "libaudiopreprocessing",
        "libaudioutils",
        "liblog",
        "libutils",
    ],
    header_libs: [
        "libaudioeffects",
        "libhardware_headers",
    ],
}

cc_test {
    name: "correlationTest",
    host_supported: false,
    srcs: ["PreProcessingCorr.cpp"],
    local_include_dirs: [
        ".",
    ],
    cflags: [
        "-DENABLE_CROSS_CORR",
        "-Wall",
        "-Werror",
        "-Wextra",
    ],
}
