// audio preprocessing unit test
cc_test {
    name: "AudioPreProcessingTest",

    vendor: true,

    relative_install_path: "soundfx",

    srcs: ["PreProcessingTest.cpp"],

    shared_libs: [
        "libaudiopreprocessing",
        "libaudioutils",
        "liblog",
        "libutils",
    ],
    header_libs: [
        "libaudioeffects",
        "libhardware_headers",
    ],
    gtest: false,
}

cc_test {
    name: "correlation",
    host_supported: true,
    srcs: ["correlation.cpp"],
    cflags: [
        "-Wall",
        "-Werror",
        "-Wextra",
    ],
    gtest: false,
}
